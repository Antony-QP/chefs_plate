<div class="main-container-booking">
    <div class="booking-background-img" style="background-image: url('https://images.unsplash.com/photo-1535980156496-87fc2cfcb832?ixid=MXwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHw%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=700&q=80')">
        <div class="booking-form" >
            <%= simple_form_for [@restaurant, @booking] do |f| %>
            <%=  f.input :start_date, as: :date %>
            <%= f.input :start_time, collection: Booking::TIMES %>
            <%= f.submit "Book Restaurant", class:"btn btn-primary"%>
            <%  end %>
        </div>
    </div>
</div>